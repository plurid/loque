{
    "name": "@plurid/loque",
    "version": "0.0.0-0",
    "description": "Human-Readable Database Based on Deon",
    "keywords": [
        "database",
        "loque"
    ],
    "author": "ly3xqhl8g9 <ly3xqhl8g9@plurid.com> (https://plurid.com)",
    "license": "SEE LICENSE IN LICENSE",
    "repository": {
        "type": "git",
        "url": "https://github.com/plurid/loque"
    },
    "bugs": {
        "email": "source@plurid.com",
        "url": "https://github.com/plurid/loque/issues"
    },
    "homepage": "https://github.com/plurid/loque/blob/master/packages/loque-javascript/README.md",
    "publishConfig": {
        "access": "public"
    },
    "engines": {
        "node": ">=12",
        "npm": ">=6"
    },
    "main": "distribution/index.js",
    "module": "distribution/index.es.js",
    "typings": "distribution/index.d.ts",
    "files": [
        "distribution"
    ],
    "scripts": {
        "test": "jest -c ./configurations/jest.config.js --coverage --verbose  ./source",
        "test.suite": "jest -c ./configurations/jest.config.js --coverage --verbose",
        "start": "rollup -c ./scripts/rollup.config.ts -w",
        "build.clean": "rm -rf `find ./distribution/ -type d -name __tests__`",
        "build.development": "rollup -c ./scripts/rollup.config.ts && yarn build.clean",
        "build.production": "rollup -c ./scripts/rollup.config.ts && yarn build.clean",
        "build": "yarn build.production",
        "prebuild": "rm -rf distribution",
        "prepublishOnly": "yarn build"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^17.0.0",
        "@types/node": "^14.14.11",
        "@typescript-eslint/eslint-plugin": "^4.9.1",
        "@typescript-eslint/parser": "^4.9.1",
        "eslint": "^7.15.0",
        "jest": "^26.6.3",
        "rollup": "^2.34.2",
        "rollup-plugin-typescript2": "^0.29.0",
        "ts-jest": "^26.4.4",
        "ts-node": "^9.1.1",
        "typescript": "^4.1.2"
    }
}
